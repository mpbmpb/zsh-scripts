alias nano=/usr/local/Cellar/nano/5.7/bin/nano
alias lx='colorls -A --sd'
alias x='colorls --sd'
alias cdp='cd ~/Projects'
alias cdg='cd ~/Projects/git'
alias mans='~/zsh-scripts/mansearch.sh'
alias cs='~/zsh-scripts/cheatsheet.sh'
alias gad='~/zsh-scripts/gitadd.sh'
alias gthrow='~/zsh-scripts/gitdiscardchanges.sh'
alias gcleanignored='~/zsh-scripts/gitcleanignoredfiles.sh'
alias gstashnpop='~/zsh-scripts/gitstashnpop.sh'
alias g2newrepo='~/zsh-scripts/gitcommit2newrepo.sh'
alias ginitrepo='~/zsh-scripts/gitinitnewrepo.sh'
alias gignore='wget -LO .gitignore https://raw.githubusercontent.com/mpbmpb/myGitignore/main/.gitignore'
alias getalias='wget -LO .useraliases https://raw.githubusercontent.com/mpbmpb/zsh-scripts/main/macalias/.useraliases'
alias .z='code ~/.zshrc'
alias go='source ~/zsh-scripts/findAndGoto.sh'
alias todos='ag -G "\.(cs|cshtml)$" todo'
alias alltodos='ag -G "\.(cs|cshtml|json|html|css|js)$" todo'
alias mmv='noglob zmv -W'
alias rider='open -na "Rider.app"'